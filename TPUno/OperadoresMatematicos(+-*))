package tpuno;

public class EjTres {

    public static void main(String[] args) {
        int a = 5;
        int b = 2;
        int suma = a + b;
        int resta = a - b;
        int mult = a * b;
        float div = a / b;
        float mod = a % b;
        System.out.println("Numero a:5| numero b:2\nSuma: "+suma+"\nResta: "+resta+"\nMultiplicacion: "+mult+"\nDivision: "+div+"\nModulo: "+mod);
    }
}
